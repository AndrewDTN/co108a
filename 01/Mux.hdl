/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=sel,out=se);
    And(a=a,b=se,out=ase);
    And(a=b,b=sel,out=bsel);
    Or(a=bsel,b=ase,out=out);
}